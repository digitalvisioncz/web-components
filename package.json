{
  "name": "@dvdevcz/components",
  "version": "0.2.2",
  "type": "module",
  "packageManager": "yarn@4.5.1",
  "workspaces": [
    "components"
  ],
  "files": [
    "README.md",
    "LICENCE",
    "package.json",
    "dist"
  ],
  "main": "dist/index.js",
  "exports": {
    "./*": "./dist/*"
  },
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "cd components && yarn dev",
    "build": "cd components && yarn build && node ../scripts/generate-components-declarations.js",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "release": "release-it"
  },
  "devDependencies": {
    "@dvdevcz/eslint": "^0.3.3",
    "@dvdevcz/stylelint": "^0.1.0",
    "@dvdevcz/typescript-config": "^0.0.3",
    "eslint": "^9.15.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "lightningcss": "^1.28.1",
    "release-it": "^17.10.0",
    "release-it-changelogen": "^0.1.0",
    "typescript": "^5.6.3",
    "typescript-plugin-css-modules": "^5.1.0",
    "vitepress": "^1.5.0"
  }
}
